[global]
ioengine=libaio #асинхронный io
direct=1
#time_based
runtime=30 #
#refill_buffers
#norandommap
#randrepeat=0
group_reporting # Комбинирование множественных заданий для отчёта
size=5G # Max read and write fio process/thread
lockmem=1G # заблокированный размер RAM для использования.
numjobs=1 #
iodepth=1 #количество потоков
#directory=/dev/rbd0
filename=/dev/rbd0

[4K_randwrite]
rw=randwrite
bs=4k
stonewall

[4K_randread]
rw=randread
bs=4k
stonewall


[4K_write]
rw=write
bs=4k
stonewall

[4K_read]
rw=read
bs=4k
stonewall

[4K_write40_read60]
rwmixread=40
bs=4k
stonewall

#----------------------

